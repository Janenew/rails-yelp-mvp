<h1>New restaurant</h1>

<%= simple_form_for @restaurant do |form| %>

<%= form.input :name %>
<%= form.input :address %>
<%= form.input :phone %>
<%= form.input :category, collection: Restaurant::CATEGORIES, prompt: "Select a type" %>
<%= form.button :submit, class: "btn btn-primary" %>

<% end %>
